#!/bin/zsh

debug-current-script "${0}" "" 2> /dev/null

export CHECK_OCCURRENCES='CHECK_OCCURRENCES'

add_ls() {
    echo $0
     [ -z "$ARR_ALIAS" ] && export ARR_ALIAS=("start");
     ARR_ALIAS+="$1"
}

showshit () {
    for i in $ARR_ALIAS
    do
        echo $i
    done
}

debug-current-script "" "${0}" 2> /dev/null
